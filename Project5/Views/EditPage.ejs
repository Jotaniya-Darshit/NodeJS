<link rel="stylesheet" href="/CSS/Style.css">
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>

<form action="/Update" method="post" enctype="multipart/form-data">
    <input type="text" value="<%=specificData.id%>" hidden name="id">
    <label for="image">Image:</label>
    <input type="file" name="img"><br>
    <img src="<%=specificData.img%>" alt="">
    <label for="name">Name:</label>
    <input type="text" value="<%=specificData.name%>" name="title"><br>
    <label for="">Descripition:</label>
    <input type="text" value="<%=specificData.Desc%>" name="Desc"><br>
    <label for="">Price:</label>
    <input type="text" value="<%=specificData.price%>" name="price"><br>
    <label for="">Release Date:</label>
    <input type="text" value="<%=specificData.releaseDate%>" name="releaseDate"><br>
    <label for="">Rating:</label>
    <input type="text" value="<%=specificData.rating%>" name="rating"><br>
    <button>Submit</button>
</form>